<head>
  <link rel="stylesheet" href="/styles/style.css">
</head>
<h1>Micro Eats</h1>
<div class="recipe_display">
  {{#each recipes}}
  <div class="recipe_box">
    <p><a href="/recipe/{{id}}">{{title}}</a> Rated:({{rating}}*)</p>
  </div>
  {{/each}}

  <div id="users_column">
    <ul>
      {{#each profiles}}
      <li><a href="/profile/{{id}}">{{name}}</a></li>
      {{/each}}
    </ul>
    <a href="/addprofile/">Add Profile</a>
  </div>
</div>